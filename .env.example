# Copy to .env and fill values. .env is gitignored.

# Cloudflare Radar (Bearer token)
CLOUDFLARE_API_TOKEN=

# VirusTotal v3 API key
VT_API_KEY=

# Shodan API key
SHODAN_API_KEY=

# AbuseIPDB API key
ABUSEIPDB_API_KEY=

# IPInfo token
IPINFO_TOKEN=

# AlienVault OTX API key (optional)
OTX_API_KEY=

# Logging level: 10=DEBUG, 20=INFO, 30=WARN, 40=ERROR
NETINTEL_LOG_LEVEL=20

# Custom User-Agent for outbound requests
NETINTEL_UA=netintel/0.1 (+https://example.invalid)

