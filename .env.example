# Copy to .env and fill values. .env is gitignored.

# Cloudflare Radar (Bearer token)
CLOUDFLARE_API_TOKEN=

# VirusTotal v3 API key
VT_API_KEY=

# Shodan API key
SHODAN_API_KEY=

# AbuseIPDB API key
ABUSEIPDB_API_KEY=

# IPInfo token
IPINFO_TOKEN=

# AlienVault OTX API key (optional)
OTX_API_KEY=

# Logging level: 10=DEBUG, 20=INFO, 30=WARN, 40=ERROR
TRIPPER_RECON_LOG_LEVEL=20

# Custom User-Agent for outbound requests (defaults to realistic browser UA if unset)
TRIPPER_RECON_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/141.0.0.0 Safari/537.36 Edg/141.0.0.0
